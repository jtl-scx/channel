{
    "name": "jtl/channel-skeleton",
    "require": {
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "jtl/channel-lib": "dev-master",
        "monolog/monolog": "^2.0",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "friendsofphp/php-cs-fixer": "^2.15",
        "mockery/mockery": "^1.2",
        "phpstan/phpstan": "^0.11.15"
    },
    "autoload": {
        "psr-4": {
            "JTL\\SCX\\Channel\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/home/patric/Dev/scx/channel-lib"
        }
    ],
    "scripts": {
        "phpcs": [
            "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist"
        ],
        "phpcs-dry": [
            "vendor/bin/php-cs-fixer fix --config=./.php_cs.dist --dry-run"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}

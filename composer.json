{
    "name": "jtl-scx/channel",
    "license": "MIT",
    "require": {
        "php": "8.3.*",
        "symfony/console": "^6.4.6",
        "symfony/dotenv": "^6.4.4",
        "jtl-scx/channel-core": "^0.102.0",
        "monolog/monolog": "^2.9.3",
        "twig/twig": "^3.9.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.54",
        "psalm/phar": "^5.23",
        "phpunit/phpunit": "^10.5.20"
    },
    "autoload": {
        "psr-4": {
            "JTL\\SCX\\Channel\\": "src/"
        }
    },
    "scripts": {
        "phpcs": [
            "vendor/bin/php-cs-fixer fix src --rules=@PSR12"
        ],
        "phpcs-dry": [
            "vendor/bin/php-cs-fixer fix src --rules=@PSR12 --dry-run"
        ],
        "psalm": [
            "vendor/bin/psalm.phar"
        ]
    },
    "prefer-stable" : true
}
